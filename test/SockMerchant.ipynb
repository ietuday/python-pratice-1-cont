    "from collections import defaultdict\n",
    " \n",
    "n = int(input().strip())\n",
    "c = map(int,input().strip().split(' '))\n",
    " \n",
    "d = defaultdict(int)\n",
    "for k in c:\n",
    "    d[k] += 1\n",
    "\n",
    "cnt = 0\n",
    "for ele in d.values():\n",
    "    cnt += ele//2\n",
    "     \n",
    "print(cnt)"
   ]
